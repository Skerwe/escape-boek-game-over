{
  "name": "escape-boek-game-over",
  "displayName": "EscapeBoekGameOver",
  "version": "0.1.0",
  "private": "true",
  "description": "This is a digital inventory companion mobile application for the book 'Escape Boek: Game Over'.",
  "author": "Indie Sagtewerke (http://indiesagtewerke.nl)",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 4173",
